{
  "name": "dsr-analytics",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "script:prototype:bridge": "ts-node ./scripts/prototype/bridge.ts",
    "script:prototype:fingerprint": "ts-node ./scripts/prototype/fingerprint.ts",
    "script:prototype:percentile": "ts-node ./scripts/prototype/percentile.ts",
    "script:prototype:asyncpool": "ts-node ./scripts/prototype/asyncpool.ts",
    "script:meta:geo": "ts-node ./scripts/meta/geo.ts",
    "script:meta:country": "ts-node ./scripts/meta/country.ts",
    "script:meta:locales": "ts-node ./scripts/meta/locales.ts",
    "script:meta": "run-p script:meta:geo script:meta:country script:meta:locales",
    "script:site": "ts-node ./scripts/site.ts",
    "script:user": "ts-node ./scripts/user.ts",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "build:server": "esbuild --outfile=./dist/server.js --bundle --platform=node ./server/index.ts",
    "build": "cross-env NODE_ENV=production run-s script:meta build:vite build:server",
    "dev:vite": "vite",
    "dev:server": "ts-node-dev ./server/index.ts",
    "dev": "cross-env NODE_ENV=development run-p dev:server dev:vite",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "test:lib": "jest --config=./jest.server.config.js ./test/lib",
    "test:web": "jest --config=./jest.web.config.js ./test/web",
    "test:e2e": "cypress open"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@popperjs/core"
      ],
      "allowedVersions": {
        "react": "17"
      }
    }
  },
  "peerDependencies": {
    "express": "~4.17.3",
    "fastify": "~3.28.0"
  },
  "dependencies": {
    "antd": "~4.19.5",
    "axios": "~0.26.1",
    "bcryptjs": "~2.4.3",
    "bootstrap": "~5.1.3",
    "clsx": "~1.1.1",
    "connect": "~3.7.0",
    "connect-history-api-fallback": "~1.6.0",
    "content-type": "~1.0.4",
    "cookie": "~0.5.0",
    "cors": "~2.8.5",
    "dayjs": "~1.11.1",
    "dotenv": "~16.0.0",
    "dsr-design": "~2.5.2",
    "echarts": "~5.3.2",
    "error-stack-parser": "~2.0.7",
    "etag": "~1.8.1",
    "express": "~4.17.3",
    "fastify": "~3.28.0",
    "is-localhost-ip": "~1.4.0",
    "isbot": "~3.4.5",
    "jose": "~4.6.1",
    "js-cookie": "~3.0.1",
    "maxmind": "~4.3.6",
    "mobx": "~6.5.0",
    "mobx-react-lite": "~3.3.0",
    "mongoose": "~6.3.0",
    "normalize.css": "~8.0.1",
    "raw-body": "~2.5.1",
    "rc-picker": "~2.6.7",
    "react": "~17.0.2",
    "react-circle": "~1.1.1",
    "react-dom": "~17.0.2",
    "react-helmet": "~6.1.0",
    "react-redux": "~8.0.0",
    "react-router-dom": "~6.3.0",
    "react-tooltip": "~4.2.21",
    "react-use": "~17.3.2",
    "request-ip": "~2.1.3",
    "router": "~1.3.6",
    "serve-static": "~1.15.0",
    "ua-parser-js": "~1.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~12.1.5",
    "@types/bcryptjs": "~2.4.2",
    "@types/connect": "~3.4.35",
    "@types/connect-history-api-fallback": "~1.3.5",
    "@types/content-type": "~1.1.5",
    "@types/cookie": "~0.5.0",
    "@types/cors": "~2.8.12",
    "@types/etag": "~1.8.1",
    "@types/jest": "~27.4.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "~16.11.27",
    "@types/react": "~17.0.44",
    "@types/react-dom": "~17.0.15",
    "@types/react-helmet": "~6.1.5",
    "@types/request-ip": "~0.0.37",
    "@types/serve-static": "~1.13.10",
    "@types/tar": "~6.1.1",
    "@types/ua-parser-js": "~0.7.36",
    "@typescript-eslint/eslint-plugin": "~5.19.0",
    "@typescript-eslint/parser": "~5.19.0",
    "@vitejs/plugin-react": "~1.3.1",
    "autocannon": "~7.6.0",
    "benchmark": "~2.1.4",
    "cross-env": "~7.0.3",
    "cypress": "~9.5.4",
    "enquirer": "~2.3.6",
    "esbuild": "~0.14.36",
    "eslint": "~8.13.0",
    "eslint-plugin-react": "~7.29.4",
    "eslint-plugin-react-hooks": "~4.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~27.5.1",
    "less": "~4.1.2",
    "npm-run-all": "~4.1.5",
    "pm2": "~5.2.0",
    "prettier": "~2.6.2",
    "sass": "~1.50.0",
    "serve": "~13.0.2",
    "tar": "~6.1.11",
    "ts-jest": "~27.1.4",
    "ts-node": "~10.7.0",
    "ts-node-dev": "~1.1.8",
    "typescript": "~4.6.3",
    "vite": "~2.9.5",
    "vite-plugin-svgr": "~1.1.0"
  }
}
